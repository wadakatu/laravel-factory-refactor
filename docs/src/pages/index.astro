  }

  .readme-content {
    background: #f7fafc;
    padding: 2rem;
    border-radius: 12px;
    border-left: 4px solid #667eea;
  }

  footer {
    text-align: center;
    padding: 2rem 0;
    color: white;
    margin-top: 3rem;
