  }

  .readme-content {
    padding: 2rem;
    max-height: 400px;
    overflow-y: auto;
  }

  .readme-content pre {
    font-family: var(--font-code, monospace);
    font-size: 0.9rem;
    line-height: 1.5;
    color: var(--text-primary);
    white-space: pre-wrap;
    word-wrap: break-word;
    margin: 0;
  }

  @media (max-width: 768px) {
    .features-grid {
      grid-template-columns: 1fr;
    }
    
    .main-description {
      padding: 1.5rem;
    }
  }

  footer {
    background: var(--primary);
    color: white;
    text-align: center;
    padding: 2rem 0;
  }

  footer p {
    margin: 0;
    opacity: 0.9;
  }
</style>