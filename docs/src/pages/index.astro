    line-height: 1.5;
    color: var(--text-secondary);
    margin: 0;
  }

  .readme-details {
    background: white;
    border-radius: 12px;
    box-shadow: 0 4px 15px rgba(0, 0, 0, 0.08);
    overflow: hidden;
  }

  .readme-details summary {
    padding: 1.5rem;
    background: var(--primary);
    color: white;
    cursor: pointer;
    font-weight: 600;
    transition: background 0.3s ease;
  }

  .readme-details summary:hover {
