{
    "name": "wadakatu/laravel-factory-refactor",
    "description": "Refactor the style of factory call from helper to static method for Laravel 8.x.",
    "keywords": [
        "laravel",
        "factory",
        "factories",
        "refactor",
        "refactoring",
        "tests"
    ],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.3",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "illuminate/console": "^6.0|^7.0|^8.0",
        "illuminate/filesystem": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Wadakatu\\LaravelFactoryRefactor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wadakatu\\LaravelFactoryRefactor\\FactoryRefactorServiceProvider"
            ]
        }
    }
}
