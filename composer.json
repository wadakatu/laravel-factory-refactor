{
    "name": "wadakatu/laravel-factory-refactor",
    "description": "Refactor the style of factory call from helper to static method for Laravel 8.x.",
    "keywords": [
        "laravel",
        "factory",
        "factories",
        "refactor",
        "refactoring",
        "tests"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/wadakatu/laravel-factory-refactor",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.3",
        "illuminate/support": "^8.0|^9.0",
        "illuminate/console": "^8.0|^9.0",
        "illuminate/filesystem": "^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.24"
    },
    "autoload": {
        "psr-4": {
            "Wadakatu\\LaravelFactoryRefactor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wadakatu\\LaravelFactoryRefactor\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wadakatu\\LaravelFactoryRefactor\\FactoryRefactorServiceProvider"
            ]
        }
    }
}
